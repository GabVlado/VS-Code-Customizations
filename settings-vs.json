{
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "terminal.integrated.fontFamily": "MesloLGS NF, consolas",
    "workbench.colorCustomizations": {
        "terminal.ansiGreen": "#05da28",
        "terminal.ansiBrightGreen": "#3bdd12"

    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.tokenColorCustomizations":{
        "textMateRules": [
            {
                "scope": "support.type.property-name.json.comments",
                "settings":{
                    "foreground":"#fffdfd"
                }
            },
            {
                "scope": "source.json meta.structure.dictionary.value string , string.quoted.double.json.comments",
                "settings": {
                    "foreground": "#FCE566"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#43eb10"
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "foreground": "#ff9f20"
                }
            },
            {
                "scope":"entity.name.type",
                "settings": {
                    "foreground": "#0A84FF",
                }
            }

        ]
    },
    "editor.bracketPairColorization.enabled": true,
    "bracketPairColorizer.depreciation-notice": false,
    "emmet.includeLanguages": {
        "erb": "html",
        "javascript": "javascriptreact"
    },
    "terminal.integrated.cursorStyle": "line",
    "explorer.compactFolders": false,
    "workbench.colorTheme": "Monokai Pro New"

}